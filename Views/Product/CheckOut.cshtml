                    @foreach (var item in Model)
                    {
                        string pic = item.ProductID.ToString() + ".jpg";
                        <div class="ro-body">
                            <div class="ro-item">
                                <div class="ro-image">
                                    <a href="#">
                                        <img src="~/Content/Product/img/products/@pic" alt="">
                                    </a>
                                </div>
                                <div>
                                    <div class="tb-beg">
                                        <a href="#">@item.ProductName</a>
                                    </div>
                                </div>
                                <div>
                                    <div class="ro-price">
                                        <span class="amount">@item.UnitPrice 元</span>
                                    </div>
                                    <div class="ro-quantity">
                                        <strong class="product-quantity">X  @item.Quantity</strong>
                                    </div>
                                    <div class="product-total">
                                        <span class="amount">@((item.UnitPrice) * (item.Quantity))元</span>
                                    </div>
                                </div>
                            </div>
                        </div>
                    }

                    <div class="ro-footer">
                        <div>
                            <p>
                                總計
                                <span>
                                    <strong>
                                        <span class="amount">@Model.Sum(p => p.Quantity * p.UnitPrice)</span>
                                    </strong>
                                </span>
                            </p>
                        </div>
                    </div>

                </div>
            </div>
            @*========================================================================================*@
        </div>
    </div>
